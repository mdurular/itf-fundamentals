    "\n",
    "prime_number = []\n",
    "\n",
    "for num in range(2,100):\n",
    "    if num > 1:\n",
    "       for i in range(2, num):\n",
    "           if (num % i) == 0:\n",
    "               break\n",
    "       else:\n",
    "            prime_number.append(num)\n",
    "print(prime_number)"
