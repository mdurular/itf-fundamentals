   "source": [
    "x = \"[[{({})}]({}))\"\n",
    "\n",
    "def isValid(s) :\n",
    "    while \"()\" in s or \"{}\" in s or \"[]\" in s :\n",
    "        s = s.replace(\"()\", \"\").replace(\"{}\", \"\").replace(\"[]\", \"\")\n",
    "    return s == \"\"\n",
    "\n",
    "isValid(x)"
   ]
